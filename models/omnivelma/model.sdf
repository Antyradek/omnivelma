<?xml version="1.0" ?>
<sdf version="1.5">
    <model name="omnivelma">
        <pose>0 0 0.101 0 0 0</pose>
        <static>false</static>
        <!--Baza całości-->
        <link name="base">
            <pose>0 -0.097 0.02 0 0 0</pose>
            <collision name="base_collision">
                <geometry>
                    <mesh>
                        <uri>model://omnivelma/meshes/collider/base.dae</uri>
                    </mesh>
                </geometry>
            </collision>
            <visual name="base_visual">
                <geometry>
                    <mesh>
                        <uri>model://omnivelma/meshes/visual/base.dae</uri>
                    </mesh>
                </geometry>
            </visual>
        </link>
        <!-- Ruchomy przód -->
        <link name="front">
            <pose>0 0.367 0.02 0 0 0</pose>
            <collision name="front_collision">
                <geometry>
                    <mesh>
                        <uri>model://omnivelma/meshes/collider/front.dae</uri>
                    </mesh>
                </geometry>
            </collision>
            <visual name="front_visual">
                <geometry>
                    <mesh>
                        <uri>model://omnivelma/meshes/visual/front.dae</uri>
                    </mesh>
                </geometry>
            </visual>
        </link>
        <!-- Koła wirtualne -->
        <link name="virtual_wheel_rr">
            <pose>0.38 -0.357 0 0 0 0</pose>
            <visual name="virtual_wheel_rr_middle">
                <geometry>
                    <mesh>
                        <uri>model://omnivelma/meshes/visual/wheel.dae</uri>
                    </mesh>
                </geometry>
                <material>
                    <diffuse>0.91 0.29 0 1</diffuse>
                </material>
            </visual>
            <visual name="virtual_wheel_rr_rollers">
                <geometry>
                    <mesh>
                        <uri>model://omnivelma/meshes/visual/wheel_l.dae</uri>
                    </mesh>
                </geometry>
                <material>
                    <diffuse>0.42 0.42 0.42 1</diffuse>
                </material>
            </visual>
        </link>
        <link name="virtual_wheel_rl">
            <pose>-0.38 -0.357 0 0 0 0</pose>
            <visual name="virtual_wheel_rl_middle">
                <geometry>
                    <mesh>
                        <uri>model://omnivelma/meshes/visual/wheel.dae</uri>
                    </mesh>
                </geometry>
                <material>
                    <diffuse>0.91 0.29 0 1</diffuse>
                </material>
            </visual>
            <visual name="virtual_wheel_rl_rollers">
                <geometry>
                    <mesh>
                        <uri>model://omnivelma/meshes/visual/wheel_r.dae</uri>
                    </mesh>
                </geometry>
                <material>
                    <diffuse>0.42 0.42 0.42 1</diffuse>
                </material>
            </visual>
        </link>
        <link name="virtual_wheel_fr">
            <pose>0.38 0.362 0 0 0 0</pose>
            <visual name="virtual_wheel_fr_middle">
                <geometry>
                    <mesh>
                        <uri>model://omnivelma/meshes/visual/wheel.dae</uri>
                    </mesh>
                </geometry>
                <material>
                    <diffuse>0.91 0.29 0 1</diffuse>
                </material>
            </visual>
            <visual name="virtual_wheel_fr_rollers">
                <geometry>
                    <mesh>
                        <uri>model://omnivelma/meshes/visual/wheel_r.dae</uri>
                    </mesh>
                </geometry>
                <material>
                    <diffuse>0.42 0.42 0.42 1</diffuse>
                </material>
            </visual>
        </link>
        <link name="virtual_wheel_fl">
            <pose>-0.38 0.362 0 0 0 0</pose>
            <visual name="virtual_wheel_fl_middle">
                <geometry>
                    <mesh>
                        <uri>model://omnivelma/meshes/visual/wheel.dae</uri>
                    </mesh>
                </geometry>
                <material>
                    <diffuse>0.91 0.29 0 1</diffuse>
                </material>
            </visual>
            <visual name="virtual_wheel_fl_rollers">
                <geometry>
                    <mesh>
                        <uri>model://omnivelma/meshes/visual/wheel_l.dae</uri>
                    </mesh>
                </geometry>
                <material>
                    <diffuse>0.42 0.42 0.42 1</diffuse>
                </material>
            </visual>
        </link>
        <!-- Środkowy przegub (zawias) -->
        <joint name="middle_joint" type="revolute">
            <parent>base</parent>
            <child>front</child>
            <!-- Pozycja względem dziecka -->
            <pose>0 -0.1 -0.02 0 0 0</pose>
            <axis>
                <xyz>0 1 0</xyz>
                <use_parent_model_frame>true</use_parent_model_frame>
            </axis>
        </joint>
        <!-- Przeguby kół -->
        <joint name="motor_rr" type="revolute">
            <parent>base</parent>
            <child>virtual_wheel_rr</child>
            <pose>-0.036 0 0 0 0 0</pose>
            <axis>
                <xyz>-1 0 0</xyz>
                <use_parent_model_frame>true</use_parent_model_frame>
            </axis>
        </joint>
        <joint name="motor_rl" type="revolute">
            <parent>base</parent>
            <child>virtual_wheel_rl</child>
            <pose>0.036 0 0 0 0 0</pose>
            <axis>
                <xyz>-1 0 0</xyz>
                <use_parent_model_frame>true</use_parent_model_frame>
            </axis>
        </joint>
        <joint name="motor_fr" type="revolute">
            <parent>front</parent>
            <child>virtual_wheel_fr</child>
            <pose>-0.036 0 0 0 0 0</pose>
            <axis>
                <xyz>-1 0 0</xyz>
                <use_parent_model_frame>true</use_parent_model_frame>
            </axis>
        </joint>
        <joint name="motor_fl" type="revolute">
            <parent>front</parent>
            <child>virtual_wheel_fl</child>
            <pose>0.036 0 0 0 0 0</pose>
            <axis>
                <xyz>-1 0 0</xyz>
                <use_parent_model_frame>true</use_parent_model_frame>
            </axis>
        </joint>
    </model>
    </sdf>
